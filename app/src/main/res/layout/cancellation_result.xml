<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ggreenbackground"
    tools:context=".CancellationResult">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="417dp"
        android:layout_height="64dp"
        android:layout_marginBottom="802dp"
        android:background="#BA2E5E30"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/account"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="349dp"
        android:layout_marginBottom="823dp"
        android:backgroundTint="#002E8B57"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/account"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="69dp"
        android:layout_height="31dp"
        android:layout_marginStart="334dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="822dp"
        android:backgroundTint="#002E8B57"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/arrow"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="129dp"
        android:layout_marginBottom="822dp"
        android:text="نتيجة المرحلة الأولى "
        android:textColor="#F4EBEB"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="122dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="123dp"
        android:layout_marginBottom="723dp"
        android:text="نتيجة مرحلة الإلغاء"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/durationCardBtn"
        android:layout_width="346dp"
        android:layout_height="106dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="242dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="518dp"
        android:padding="4dp"
        app:cardBackgroundColor="#D5FFFFFF"
        app:cardCornerRadius="30dp"
        app:cardElevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/durationCard_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="المدة:"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="306dp"
        tools:layout_editor_absoluteY="279dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/durationCard_Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="00:00"
        android:textColor="@color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="279dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/LoudnessCard_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="مستوى الصوت:"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="433dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/loudnessCard_Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="0%"
        android:textColor="@color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="193dp"
        tools:layout_editor_absoluteY="433dp" />

    <androidx.cardview.widget.CardView
        android:id="@+id/loudnessCard"
        android:layout_width="346dp"
        android:layout_height="106dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="396dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="364dp"
        android:padding="4dp"
        app:cardBackgroundColor="#D5FFFFFF"
        app:cardCornerRadius="30dp"
        app:cardElevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/pitchCard_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="حدة الصوت:"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="548dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/pitchCard_Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="0 hrtz "
        android:textColor="@color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="548dp" />

    <androidx.cardview.widget.CardView
        android:id="@+id/pitchCard"
        android:layout_width="346dp"
        android:layout_height="106dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="548dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="212dp"
        android:padding="4dp"
        app:cardBackgroundColor="#D5FFFFFF"
        app:cardCornerRadius="30dp"
        app:cardElevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.cardview.widget.CardView>






</androidx.constraintlayout.widget.ConstraintLayout>